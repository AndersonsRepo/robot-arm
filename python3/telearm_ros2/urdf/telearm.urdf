<!-- Minimal 5-DOF chain with joint names matching example_model() -->
<robot name="telearm_5dof">
  <link name="base_link"/>
  <joint name="base_yaw" type="revolute">
    <parent link="base_link"/><child link="link1"/>
    <origin xyz="0 0 0.10" rpy="0 0 0"/>
    <axis xyz="0 0 1"/><limit lower="-3.1416" upper="3.1416" effort="5" velocity="2.0"/>
  </joint>
  <link name="link1"/>
  <joint name="base_pitch" type="revolute">
    <parent link="link1"/><child link="link2"/>
    <origin xyz="0.05 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/><limit lower="-1.5708" upper="1.5708" effort="5" velocity="2.0"/>
  </joint>
  <link name="link2"/>
  <joint name="elbow" type="revolute">
    <parent link="link2"/><child link="link3"/>
    <origin xyz="0.15 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/><limit lower="-2.3562" upper="2.3562" effort="5" velocity="2.0"/>
  </joint>
  <link name="link3"/>
  <joint name="wrist_pitch" type="revolute">
    <parent link="link3"/><child link="link4"/>
    <origin xyz="0.12 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/><limit lower="-2.3562" upper="2.3562" effort="3" velocity="2.0"/>
  </joint>
  <link name="link4"/>
  <joint name="wrist_roll" type="revolute">
    <parent link="link4"/><child link="tool0"/>
    <origin xyz="0.05 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/><limit lower="-3.1416" upper="3.1416" effort="3" velocity="2.0"/>
  </joint>
  <link name="tool0"/>
</robot>
